{
  "name": "FuturePredictorAPI",
  "version": "1.0.0",
  "description": "This is a API that predicts your future with a 50% chance of success",
  "scripts": {
    "dev": "nodemon",
    "prestart:prod": "rm -rf dist && tsc",
    "start": "node dist/server.js",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guastallaigor/future-predictor-api.git"
  },
  "keywords": [],
  "author": "Igor Guastalla <limaguastallaigor@@gmail.com>",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/guastallaigor/future-predictor-api/issues"
  },
  "homepage": "https://github.com/guastallaigor/future-predictor-api#readme",
  "dependencies": {
    "@nestjs/common": "^5.0.0-beta.6",
    "@nestjs/core": "^5.0.0-beta.6",
    "@nestjs/microservices": "^5.0.0-beta.6",
    "@nestjs/testing": "^5.0.0-beta.6",
    "@nestjs/websockets": "^5.0.0-beta.6",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "class-transformer": "^0.1.8",
    "class-validator": "^0.7.2",
    "pg": "^7.3.0",
    "reflect-metadata": "^0.1.12",
    "fastify-formbody": "^2.0.0",
    "rxjs": "^6.0.0",
    "slug": "^0.9.1",
    "typeorm": "^0.1.0",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/slug": "^0.9.0",
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.1.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.3.2",
    "webpack": "^4.2.1",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "json",
      "js"
    ],
    "rootDir": "src",
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".*\\.spec.(ts|tsx|js)$",
    "coverageDirectory": "../coverage"
  }
}
